<div class="grid">
    <div class="col-1-1">
        <div class="module center">
            <h1>Log In</h1>
        </div>
    </div>
    <div class="col-1-1">
        <div class="module">
            <form action="/logIn" method="POST">
                <div class="center">
                    <label for="username">User: </label>
                    <input type="text" name="username" class="text">
                </div><br>
                <div class="center">
                    <label for="password">Password: </label>
                    <input type="text" name="password" class="text">
                </div><br>
                <div>
                    <button type="submit" class="centerButton">Log In</button>
                </div>
            </form>
        </div>
    </div>
</div>